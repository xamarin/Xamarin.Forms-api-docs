{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "_missing-apis.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "dest":"api/xamarin-media"
      }
    ],
    "overwrite": {
      "files": [
        "api/overwrites/**/*.md"
      ]
    },
    "globalMetadata": {
      "ms.service": "xamarin",
      "apiPlatform": "dotnet",
      "breadcrumb_path": "/dotnet/XamarinFormsAPI-breadcrumb/toc.json",
      "author": "dotnet-bot",
      "ms.author": "dotnetcontent",
      "manager": "crdun",
      "ms.date": "03/13/2019",
      "searchScope": ["Xamarin.Forms API"],
      "uhfHeaderId": "MSDocsHeader-Xamarin",
      "ms.topic": "managed-reference",
      "ms.technology":"xamarin-forms"
    },
    "fileMetadata": {
      "author": {
        "api/**.yml": "dotnet-bot"
      }
    },
    "template": [],
    "isCanonicalUrlWithMoniker": true,
    "dest": "_site",
    "xref": [
      "_missing-apis.yml"
    ],
    "lruSize": 0
  }
}
